.btn {font-family: "Microsoft Yahei"; border-radius: 2px;}
.btn-primary {
    background-color: #F25A43;
    background-image: -moz-linear-gradient(top, #F5634C, #F25A43);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#F5634C), to(#F25A43));
    background-image: -webkit-linear-gradient(top, #F5634C, #F25A43);
    background-image: -o-linear-gradient(top, #F5634C, #F25A43);
    background-image: linear-gradient(to bottom, #F5634C, #F25A43);
    background-repeat: repeat-x;
    border-color: #F25A43 #F25A43 #002a80;
    border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffF5634C', endColorstr='#ffF25A43', GradientType=0);
    filter: progid:DXImageTransform.Microsoft.gradient(enabled=false);
}
.btn-primary:hover, .btn-primary:focus, .btn-primary:active, .btn-primary.active, .btn-primary.disabled, .btn-primary[disabled] {
    background-color: #F25A43;
}

