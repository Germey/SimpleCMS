.fx_banner_inner{position:relative;height:300px}

.fx_banner_img{width:20000px;}
.fx_banner_img li{float:left;width:1000x;position:relative}
.fx_banner_link{position:absolute;top:0;left:0;width:100%;height:502px;cursor:pointer}

.fx_banner .child{position:absolute;width:1903px;height:502px}
.fx_banner_common{position:relative;display:none;}
.fx_banner_common a{position:absolute;margin-top:130px;/*margin-top:-315px;*/width:45px;height:119px;cursor:pointer;display:block;overflow:hidden;opacity:0.5;filter:alpha(opacity=50);}
.fx_banner_pre{left:4px;background:url(./images/banner_prev.png) no-repeat;}
.fx_banner_next{right:4px;background:url(./images/banner_next.png) no-repeat;}
.fx_banner_pre:hover,.fx_banner_next:hover{opacity:1;filter:alpha(opacity=100)}
.fx_banner .control{width:200px;position:absolute;top:270px;left:66%;text-align:center}
.fx_banner .control li{display:inline-block;*display:inline;*zoom:1;margin:0 5px;width:12px;height:12px;border-radius:50%;cursor:pointer;background:#CCC}
.fx_banner .control li a{display:none}
.fx_banner .control li.current{background:#A80204}
.fx_banner .banner_des {position: absolute;top:250px;left:10px;width:290px;height: 100px;background: #B90F16;color:#fff;padding:10px 20px;border-radius: 5px;opacity:0.9;filter:alpha(opacity=90);}
.fx_banner .banner_des h5 {margin:0;}
.fx_banner .banner_des p {margin:0;}
.fx_banner .banner_des a{color:#fff;}
.fx_banner .main-img {width:960px;}
.fx_banner .main-img img{width:540px; height: 300px;}

.fx_banner .main-img table td{vertical-align: top;}
.fx_banner .main-img table td.img{width: 540px;}
.fx_banner .main-img table td.text{padding:0 20px;}
.fx_banner .main-img table td h5{font-size: 16px}
.fx_banner .main-img table td .desc{font-size:13px; color:#666; line-height: 24px}

